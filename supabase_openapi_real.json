{
  "openapi": "3.0.0",
  "info": {
    "title": "Supabase Todo API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://unharping-paulene-overslowly.ngrok-free.dev/rest/v1",
      "description": "Supabase API"
    }
  ],
  "paths": {
    "/todos": {
      "get": {
        "summary": "Get Todos",
        "parameters": [
          {
            "name": "select",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "example": "*"
          }
        ],
        "responses": {
          "200": {
            "description": "List of todos"
          }
        }
      },
      "post": {
        "summary": "Create Todo",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string"
                    },
                    "done": {
                      "type": "boolean"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      }
    }
  }
}